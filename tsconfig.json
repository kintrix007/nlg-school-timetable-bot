{
    "compilerOptions": {
        "target": "es6",
        "lib": ["es6"],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "isolatedModules": false,
        "noEmit": false
    }
}
